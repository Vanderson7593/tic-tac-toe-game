{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "importHelpers": true,
    "noImplicitAny": true,
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.d.ts", "*.d.ts", "**/*.ts", "**/*.tsx", "**/*.js", "**/*.hbs"],
  "exclude": ["node_modules"]
}
